{
  "name": "elegant-cli",
  "version": "1.1.4",
  "description": "A utility-first CMS framework for rapidly building expressive and elegant static content web applications.",
  "scripts": {
    "dev": "cd source/elegant-docs && npm run dev",
    "test": "jest --watch --verbose ./integrations --detectOpenHandles --forceExit",
    "test:ci": "jest --ci ./integrations --detectOpenHandles --forceExit && cd source/elegant-docs && npm run test:ci",
    "test:elegant": "cd source/elegant-docs && npm run test --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elegantframework/elegant.git"
  },
  "keywords": [
    "next",
    "react",
    "docusaurus",
    "storybook",
    "tailwindcss",
    "jest"
  ],
  "author": "Brandon Owens",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/elegantframework/elegant/issues"
  },
  "homepage": "https://github.com/elegantframework/elegant#readme",
  "dependencies": {
    "chalk": "^4.1.2",
    "clear": "^0.1.0",
    "commander": "^10.0.0",
    "figlet": "^1.5.2",
    "path": "^0.12.7"
  },
  "bin": {
    "elegant-docs-init": "./source/init.js"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.0",
    "@types/react-test-renderer": "^18.0.0",
    "child_process": "^1.0.2",
    "jest": "^29.5.0",
    "nodemon": "^2.0.21",
    "react-test-renderer": "^18.2.0",
    "shelljs": "^0.8.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  }
}
